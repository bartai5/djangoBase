{
    "builds": [
        {
            "src": "djangoBase/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
        }
    ],
    "routers":[
        {
            "src": "/(.*)",
            "dest": "djangoBase/wsgi.py"
        }
    ]
}